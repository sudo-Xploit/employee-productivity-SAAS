[run]
source = app
omit = 
    */tests/*
    */migrations/*
    */alembic/*
    */venv/*
    */env/*
    */.venv/*
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError
    raise ValueError
    
[html]
directory = htmlcov
